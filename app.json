{
  "expo": {
    "name": "AppNovaHome",
    "slug": "AppNovaHome",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.iterative.OmoLavanderia"
    },
    "web": {
      "output": "server",
      "bundler": "metro",
      "favicon": "./assets/favicon.png"
    }
  }
}
